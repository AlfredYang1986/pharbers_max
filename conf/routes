# Routes
# This file defines all application routes (Higher priority routes first)

# 用户登入
POST    /auth/authWithPassword                       controllers.auth.AuthController.auth_with_password

POST     /api/user/push                               controllers.users.UsersController.pushUser
POST     /api/user/pop                                controllers.users.UsersController.popUser
POST     /api/user/query                              controllers.users.UsersController.queryUser
POST     /api/user/query/multi                        controllers.users.UsersController.queryUserMulti
#POST    /user/update                                 controllers.auth.AuthController.auth_with_password
#POST    /user/search                                 controllers.auth.AuthController.auth_with_password

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file                               controllers.Assets.versioned(path="/public", file: Asset)
