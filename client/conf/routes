# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~
GET     /test                       controllers.Application.test
GET     /                           controllers.Application.login
GET     /landing                    controllers.Application.landing
GET		/login						controllers.Application.login
POST	/login						controllers.Application.login
GET		/index						controllers.Application.index
POST	/index						controllers.Application.index
GET		/register					controllers.Application.register
GET		/error404					controllers.Application.error404
GET		/error500					controllers.Application.error500
GET		/lockscreen					controllers.Application.lockScreen
GET		/forgotpassword				controllers.Application.forgotPassword
GET		/emptypage					controllers.Application.emptyPage
# Core Function
GET		/filesupload				controllers.Application.filesUpload
GET		/samplecheck				controllers.Application.sampleCheck
GET		/modeloperation				controllers.Application.modelOperation
GET		/resultcheck				controllers.Application.resultCheck
GET		/resultquery				controllers.Application.resultQuery
POST 	/resultquery/search			controllers.business.ResultQueryController.resultQueryAjaxCall
POST	/samplecheck/check			controllers.business.SampleCheckController.sampleCheckAjaxCall
POST    /samplecheck/line           controllers.business.SampleCheckController.sampleCheckAjaxChartsLineCall
POST    /samplecheck/plot           controllers.business.SampleCheckController.sampleCheckAjaxChartsPlotCall
POST	/modeloperation/operation	controllers.business.ModelOperationController.mondelOperationAjaxCall
POST	/resultquery/fileexport	    controllers.business.FileExportController.fileExportAjaxCall
POST    /resultquery/tempexport	    controllers.business.TempExportController.tempExportAjaxCall
POST	/login/start			    controllers.business.LoginController.Login
POST	/uploadfiles				controllers.business.FilesUploadController.filesUploadAjaxCall
GET     /resultquery/files/:name    controllers.foptController.exportFile(name)
POST    /callcheckexcel             controllers.business.CallAkkaHttpController.callHttpCheckExcelAjaxCall
POST    /callrunmodel               controllers.business.CallAkkaHttpController.callHttpRunModelAjaxCall
POST    /callfileexport             controllers.business.CallAkkaHttpController.callHttpFileExportAjaxCall

#upload
GET     /pharbers/files/:name       controllers.foptController.downloadFile(name)
POST	/pharbers/files/upload		controllers.foptController.uploadFile

GET     /manage                     controllers.Application.manageUploadFile
POST	/manage/uploadHospitalFile	controllers.foptController.uploadHospitalFile
POST	/manage/writedata			controllers.manage.FilesUploadController.filesUploadAjaxCall

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
