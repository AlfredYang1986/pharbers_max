<div style="display: none;" class="progresstier">
    <div class="progress">&nbsp;</div>
    <div class="widgets">
        <div class="shadow">
            <div id="chart" class="chart-container"> </div>
        </div>
    </div>
</div>
<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-12">
        <h2>文件上传</h2>
        <ol class="breadcrumb">
            <li>
                <a href="index.html">首页</a>
            </li>
            <li class="active">
                <strong>文件上传</strong>
            </li>
        </ol>
    </div>
</div>
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>CPA<small> 模块说明性文字预留</small></h5>
                </div>
                <div class="ibox-content">
                    <ul>
                        <li style="list-style-type:none;">
                            <div class="uploder-container">
                                <div class="phauploder">
                                    <div class="queueList">
                                        <div class="placeholder">
                                            <div class="filePicker"></div>
                                            <p>将文件拖拽到这里，最多上传16个文件</p>
                                        </div>

                                    </div>
                                    <div class="statusBar" style="display:none;">
                                        <div class="btns">
                                            <div class="jxfilePicker" style="float:left"></div>
                                            <div class="uploadBtn" style="float:left">开始上传</div>
                                        </div>
                                        <div class="info"></div>
                                    </div>
                                </div>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>GYCX<small> 模块说明性文字预留</small></h5>
                </div>
                <div class="ibox-content">
                    <ul>
                        <li style="list-style-type:none;">
                            <div class="uploder-container">
                                <div class="phauploder">
                                    <div class="queueList">
                                        <div class="placeholder">
                                            <div class="filePicker"></div>
                                            <p>将文件拖拽到这里，最多可上传16个文件</p>
                                        </div>

                                    </div>
                                    <div class="statusBar" style="display:none;">
                                        <div class="btns">
                                            <div class="jxfilePicker" style="float:left"></div>
                                            <div class="uploadBtn" style="float:left">开始上传</div>
                                        </div>
                                        <div class="info"></div>
                                    </div>
                                </div>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        {{#if auth}}
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Manager</h5>
                </div>
                <div class="ibox-content">
                    <ul>
                        <li style="list-style-type:none;">
                            <div class="uploder-container">
                                <div  class="phauploder">
                                    <div class="queueList">
                                        <div class="placeholder">
                                            <div class="filePicker"></div>
                                            <p>将文件拖拽到这里，最多上传16个文件</p>
                                        </div>
                                    </div>
                                    <div class="statusBar" style="display:none;">
                                        <div class="btns">
                                            <div class="jxfilePicker" style="float:left"></div>
                                            <div class="uploadBtn" style="float:left">开始上传</div>
                                        </div>
                                        <div class="info"></div>
                                    </div>
                                </div>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Hospital</h5>
                </div>
                <div class="ibox-content">
                    <div class="row">
                        <div class="col-sm-2 col-md-2">
                            <div class="form-group">
                                <label class="control-label" for="status">数据类型</label>
                                <select data-name="search-result-market" class="form-control">
                                    {{#each markets as |market|}}
                                        <option>{{market}}</option>
                                    {{/each}}
                                </select>
                            </div>
                        </div>
                        <div class="col-sm-2 col-md-2">
                            <div class="form-group">
                                <label class="control-label" for="status">年份</label>
                                <select data-name="search-result-market" name="timestamp" class="form-control">
                                    <option>2013</option>
                                    <option>2014</option>
                                    <option>2015</option>
                                    <option>2016</option>
                                    <option selected>2017</option>
                                    <option>2018</option>
                                    <option>2019</option>
                                    <option>2020</option>
                                </select>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="ibox-content">
                    <ul>
                        <li style="list-style-type:none;">
                            <div class="uploder-container">
                                <div class="phauploder">
                                    <div class="queueList">
                                        <div class="placeholder">
                                            <div class="filePicker"></div>
                                            <p>将文件拖拽到这里，最多上传16个文件</p>
                                        </div>

                                    </div>
                                    <div class="statusBar" style="display:none;">
                                        <div class="btns">
                                            <div class="jxfilePicker" style="float:left"></div>
                                            <div class="uploadBtn" style="float:left">开始上传</div>
                                        </div>
                                        <div class="info"></div>
                                    </div>
                                </div>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        {{/if}}
        <div class="col-lg-12">
            <div class="ibox-content">
                <div class="row">
                    <div id="resultDiv" class='col-lg-6 text-left'>
                    </div>
                    <div class="col-lg-6 text-right">
                        <button type="button" class="btn btn-primary btn-sm" id="f_nextstepBtn">下一步</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="generate_panel_file" class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>上传文件检查</h5>
                </div>
                <div class="ibox-content">
                    <div class="row">
                        <label id="generate_panel_content"></label>
                        <div id="resultDiv" hidden class='col-lg-12 text-left'></div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-12">
            <div class="ibox-content">
                <div class="row">
                    <div class="col-lg-12 text-right">
                        <button type="button" class="btn btn-primary btn-sm" id="s_prevstepBtn">上一步</button>
                        <button type="button" class="btn btn-primary btn-sm" id="s_nextstepBtn">下一步</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="generate_sample_data" class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>生成panel文件</h5>
                </div>
